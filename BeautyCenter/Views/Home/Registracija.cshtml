<form asp-controller="Home" asp-action="Registracija" method="post">
    <fieldset>
        <legend>Регистрација</legend>

        <div class="form-group">
            <label for="exampleInputEmail1">Име и презиме</label>
            <input type="text" class="form-control" name="imeIprezime" >

        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">Email адреса</label>
            <input type="email" class="form-control" name="email" >

        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Лозинка</label>
            <input type="password" class="form-control" name="lozinka">
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Телефонски број</label>
            <input type="number" class="form-control" name="telBroj">
        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Општина</label>
            <input type="text" class="form-control" name="opshtina">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </fieldset>
</form>
