@{
    ViewData["Title"] = "Login";
}
<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/lib/jquery-validate/jquery.validate.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
<script src="~/lib/jquery-validate/jquery.validate.min.js"></script>
<script src="~/lib/jquery-validate/additional-methods.js"></script>
<form asp-action="LoginUser" method="post">
    <fieldset>
        <div class="form-group">
            <label for="exampleInputEmail1">E-mail адреса</label>
            <input type="email" class="form-control" name="userName" id="userName" aria-describedby="emailHelp" placeholder="Enter email">

        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Лозинка</label>
            <input type="password" class="form-control" name="password" id="password" placeholder="Password">
        </div>
        <button type="submit" class="btn btn-secondary">Логин</button>
    </fieldset>
</form>