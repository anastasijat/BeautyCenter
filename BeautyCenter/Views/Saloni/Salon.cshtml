@model IEnumerable<BeautyCenter.Models.Saloni>
-
@{
    ViewData["Title"] = "Search";
}

    <table class="table">
        <tr>
            <th>Име</th>
            <th>Телефонски број</th>
            <th>Улица</th>
            <th>Општина</th>

        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelIdem => item.ImeSalon)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TelBrojSalon)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Ulica)
                    @Html.DisplayFor(modelItem=>item.Broj)
                </td>
                <td>
                    @Html.DisplayFor(modelItem=>item.IdOpshtina)
                </td>
            </tr>
        }
    </table>
